{
  "manifest_version": 3,
  "name": "GMeet Summarizer",
  "version": "1.0",
  "description": "Summarize Google Meet in real-time and export PDF",
  "permissions": ["tabCapture", "storage"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "index.html",
    "default_icon": "icon.png"
  },
  "icons": {
    "128": "icon.png"
  }
}
